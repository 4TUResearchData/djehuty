[metadata]
name = djehuty
version = @VERSION@
author = Roel Janssen
author_email = r.r.e.janssen@tudelft.nl
description =
long_description = file: README.md
long_description_content_type = text/markdown
url = https://gitlab.tudelft.nl/4tu.researchdata/djehuty

[options]
package_dir =
    = src
packages = find:
python_requires = >=3.6
include_package_data = True

[options.entry_points]
console_scripts =
    djehuty = djehuty.ui:main

[options.packages.find]
where = src

[sdist]
owner = root
group = root
