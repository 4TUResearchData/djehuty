{% extends "prefixes.sparql" %}
{% block query %}
PREFIX col: <sg://0.99.12/table2rdf/Column/>
PREFIX sg:  <https://sparqling-genomics.org/0.99.12/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT COUNT(DISTINCT ?articles) AS ?articles

WHERE {
  GRAPH <{{state_graph}}> {
    <{{collection_uri}}> rdf:type                         sg:Collection ;
                         col:articles/rdf:rest*/rdf:first ?articles .
  }
}
{% endblock %}
