{% extends "prefixes.sparql" %}
{% block query %}
DELETE {
  GRAPH <{{state_graph}}> {
    ?article  ?predicate             ?object .
  }
}
WHERE {
  GRAPH <{{state_graph}}> {
    ?article  rdf:type               sg:Article .
    ?article  col:article_version_id {{article_version_id}} .
    ?article  col:account_id         {{account_id}} .
    ?article  ?predicate             ?object .
  }
}
{% endblock %}
