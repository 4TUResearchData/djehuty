{% extends "prefixes.sparql" %}
{% block query %}
SELECT DISTINCT ?uuid
WHERE {
  GRAPH <{{state_graph}}> {
    ?author     rdf:type             djht:Author ;
                djht:orcid_id        {{orcid | safe}} ;
                djht:account         ?account .
  }
  BIND(STRAFTER(STR(?account), "account:") AS ?uuid)
}
{% endblock %}
