{% extends "prefixes.sparql" %}
{% block query %}
DELETE {
  GRAPH <{{state_graph}}> {
    ?session  ?predicate     ?object .
  }
}
WHERE {
  GRAPH <{{state_graph}}> {
    ?session    rdf:type       sg:Session .
    ?session    col:account_id ?account_id .
    ?session    col:id         ?id .
    ?session    ?predicate     ?object .

    FILTER (?id = {{session_id}})
    FILTER (?account_id = {{account_id}})
  }
}
{% endblock %}
