{% extends "prefixes.sparql" %}
{% block query %}
SELECT ?downloads ?views ?shares ?cites
WHERE {
  GRAPH <{{state_graph}}> {
    ?container            a                    djht:ArticleContainer ;
                          col:article_id       {{article_id}} .
    OPTIONAL { ?container col:total_downloads  ?downloads . }
    OPTIONAL { ?container col:total_views      ?views .     }
    OPTIONAL { ?container col:total_shares     ?shares .    }
    OPTIONAL { ?container col:total_cites      ?cites .     }
  }
}
{% endblock %}
