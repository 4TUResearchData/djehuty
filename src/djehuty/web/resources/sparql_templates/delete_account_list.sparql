{% extends "prefixes.sparql" %}
{% block query %}
DELETE {
  GRAPH <{{state_graph}}> {
    ?account     djht:{{predicate}}   ?item_list .
  }
}
WHERE {
  GRAPH <{{state_graph}}> {
    ?account    rdf:type             djht:Account ;
                djht:account_id       ?account_id ;
                djht:{{predicate}}    ?item_list .
  }
  FILTER (?account_id = {{account_id}})
}
{% endblock %}
