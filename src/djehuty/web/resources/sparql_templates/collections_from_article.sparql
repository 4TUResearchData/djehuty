{% extends "prefixes.sparql" %}
{% block query %}
SELECT ?collection_id ?title
WHERE {
  GRAPH <{{state_graph}}> {
    ?collection rdf:type          sg:Collection ;
                col:collection_id ?collection_id ;
                col:title         ?title ; 
                col:is_public     1 .
    ?link       rdf:type          sg:CollectionArticle ;
                col:article_id    {{article_id}} ;
                col:collection_id ?collection_id .
  }   
}
{% endblock %}
